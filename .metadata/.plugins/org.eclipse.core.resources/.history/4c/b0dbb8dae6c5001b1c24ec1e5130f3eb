import java.io.*;


public class BinaryFileRead {
	public static void main(String[] args) {
		byte [] b= {};
		
		try {
			FileInputStream fi = new FileInputStream("c:\\¸ð±¤À±\\2021-1ÇÐ±â\\ÀÚ¹Ù\\BinaryFileWrie.out");
			int n = 0, c;
			
			while((c = fi.read()) != -1) {
				b[n] = (byte)c;
				
				n++;
			}
			
			for(int i = 0;i<b.length;i++) {
				System.out.print(b[i]);
			}
			
			fi.close();
		}
		catch(IOException e) {
			System.out.println();
		}
	}
}
