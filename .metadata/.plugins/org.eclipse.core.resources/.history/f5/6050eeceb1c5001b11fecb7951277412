import java.io.*;
public class RileReaderEx {
	public static void main(String[] args) {
		FileReader in = null;
		try {
			in = new FileReader("c:\\모광윤\\2021-1학기\\자바\\Hello.txt");
			int c;
			while((c=in.read())!= -1) {
				System.out.print((char)c);
			}
		}
		catch(IOException e) {
			System.out.println("입출력 오류");
		}
	}
}
